
<h1 class="m-5 mb-4">Applicants</h1>
<div style="display: flex; justify-content: center; align-items: center;">
    <table class="table table-dark m-5">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Name</th>
                <th scope="col">Email</th>
                <th scope="col">Contact</th>
                <th scope="col">Resume</th>
            </tr>
        </thead>
        <tbody>
            <% candidates.forEachcandidate=> { %>
                <tr>
                    <th scope="row">
                        <%=  %>
                    </th>
                    <td>
                        <%= candidate.name %>
                    </td>
                    <td>
                        <%= product.email %>
                    </td>
                    <td><%= candidate.contact %>
                    </td>
                    <td>
                        
                        <button class="btn btn-primary onclick="deleteProduct('<%= candidate.id %>')">View</button>
                      
                    </td>
                </tr>
                <% }) %>
        </tbody>
    </table>
</div>
